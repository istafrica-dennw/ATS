{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "framework": "create-react-app",
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://ats-iwgo.onrender.com/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "REACT_APP_API_URL": "@react_app_api_url",
    "REACT_APP_SOCKET_URL": "@react_app_socket_url",
    "REACT_APP_IAA_URL": "@react_app_iaa_url",
    "REACT_APP_IAA_FRONTEND_URL": "@react_app_iaa_frontend_url",
    "REACT_APP_IAA_CLIENT_ID": "@react_app_iaa_client_id",
    "REACT_APP_IAA_CLIENT_SECRET": "@react_app_iaa_client_secret"
  }
}